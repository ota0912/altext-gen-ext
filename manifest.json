{
  "manifest_version": 3,
  "name": "Altext",
  "description": "Generates alt text for images lacking it",
  "icons": {
    "128": "altext-gen.png"
  },
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
